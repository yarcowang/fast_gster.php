{
    "name": "yarco/fast_gster",
    "description": "A simple way to add getter/setter feature by using attribute grammar",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "symfony/expression-language": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yarco\\FastGster\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yarco Wang<yarco.wang@gmail.com>"
        }
    ]
}
